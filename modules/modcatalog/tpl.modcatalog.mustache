<script id="catalogTpl" type="text/html">

	<table class="list_header">
		<thead class="sticked--top">
		<TR class="header_contaner">
			<TH class="w15 hidden-netbook">
				<input type="checkbox" name="che" id="che" onclick="checkb()"/>
			</TH>
			{{#mcArtikul}}
			<TH class="yw80 text-left">
				<div class="ellipsis hand" id="x-artikul" onclick="changesort('artikul')" title="Изменить порядок вывода">Артикул</div>
			</TH>
			{{/mcArtikul}}
			<TH class="yw60 text-center"><B></B></TH>
			<TH class="w30 text-center"></TH>
			<TH class="yw250 text-left">
				<div class="ellipsis hand" id="x-title" onclick="changesort('title')" title="Изменить порядок вывода">Название позиции</div>
			</TH>
			{{#price_1}}
			<TH class="yw100 text-left">
				<div class="ellipsis hand" id="x-price_1" onclick="changesort('price_1')" title="Изменить порядок вывода">{{price_1}}, {{valuta}}</div>
			</TH>
			{{/price_1}}
			<TH class="yw60 text-center"><b>Кол-во</b></TH>
			<TH class="yw40 text-left"><b>Ед.изм.</b></TH>
			<TH class="yw60 text-center"><b>Резерв</b></TH>
			<TH class="text-left"><b>Описание</b></TH>
			<TH class="w5"></TH>
		</TR>
		</thead>
		<tbody>
		{{#list}}
		<tr class="ha th55" id="catalog{{id}}">
			<TD class="hidden-netbook">
				<input name="ch[]" type="checkbox" class="mc" id="ch[]" value="{{id}}"/>
			</TD>
			{{#mcArtikul}}
			<TD><span class="ellipsis"><B>{{artikul}}</B></span></TD>
			{{/mcArtikul}}
			<TD {{{image}}}></TD>
			<TD class="text-center">
				<A href="javascript:void(0)" onclick="window.open('card.modcatalog.php?n_id={{id}}')"><i class="icon-archive broun"></i></A>
			</TD>
			<TD>
				<SPAN class="ellipsis" title="{{title}}">
					<a href="javascript:void(0)" onClick="doLoad('modules/modcatalog/form.modcatalog.php?action=view&n_id={{id}}');"><B>{{title}}</B></a>
				</SPAN><br>
				{{#cat}}<span class="smalltxt blue">{{cat}}</span>{{/cat}}
			</TD>
			{{#price_1}}
			<TD class="text-right" title="{{price1}} {{valuta}}/{{edizm}}">
						<span>
							<b class="blue">{{price1}}</b>
							{{#mcKolEdit}}&nbsp;<a href="javascript:void(0)" onClick="doLoad('modules/modcatalog/form.modcatalog.php?n_id={{id}}&action=editone&tip=price');" title="Изменить" class="dleft1 gray"><i class="icon-pencil smalltxt"></i></a>{{/mcKolEdit}}
						</span>
			</TD>
			{{/price_1}}
			<TD class="text-right Bold"><span>{{kol}}</span></TD>
			<TD>
				<div class="ellipsis">{{{edizm}}}</div>
			</TD>
			<TD class="text-right Bold"><span>{{{res}}}</span></TD>
			<TD>
				<div title="{{content}}" class="ellipsis">{{content}}</div>
			</TD>
			<TD>
				<div class="action--container">
					<div class="action--block">
						<A href="javascript:void(0)" onClick="doLoad('modules/modcatalog/form.modcatalog.php?n_id={{id}}&action=view');" title="Просмотр" class="gray green"><i class="icon-eye broun"></i></A>
						{{#mcCoordinator}}
						<A href="javascript:void(0)" onClick="doLoad('modules/modcatalog/form.modcatalog.php?n_id={{id}}&action=edit');" title="Изменить" class="gray blue"><i class="icon-pencil blue"></i></A>
						<A href="javascript:void(0)" onClick="cf=confirm('Вы действительно хотите удалить?');if (cf)delCat('{{id}}')" title="Удалить" class="gray red"><i class="icon-cancel red"></i></A>
						{{/mcCoordinator}}
					</div>
				</div>
			</TD>
		</tr>
		{{/list}}
		</tbody>
	</TABLE>
	<div class="h60" style="height:60px"></div>

</script>

<script id="zayavkaTpl" type="text/html">
