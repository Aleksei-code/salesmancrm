						{{/isDo}}
						{{#isDo}}
						<!--<A href="javascript:void(0)" onClick="cf=confirm('Вы действительно хотите изменить ВЫПОЛНЕННУЮ заявку ');if (cf)doLoad('modules/modcatalog/form.modcatalog.php?id={{id}}&action=editzayavka');" class="gray blue" title="Изменить"><i class="icon-pencil blue"></i></A>
		<A href="javascript:void(0)" onClick="cf=confirm('Вы действительно хотите удалить запись?\nБудет удалена ВЫПОЛНЕННАЯ заявка и связанные позиции заявки');if (cf)removeZayavka('{{id}}');" class="gray red" title="Удалить"><i class="icon-cancel-circled red"></i></A>-->
						{{/isDo}}
						{{/editor}}
						{{/change}}
					</div>
				</div>
			</TD>
		</tr>
		{{/list}}
		</tbody>
	</TABLE>

	<div class="block h60" style="height:60px"></div>

