						{{/isorder}}
						{{#change}}
						{{#editor}}
						<A href="javascript:void(0)" onClick="doLoad('modules/modcatalog/form.modcatalog.php?id={{id}}&action=editzayavka');" class="gray blue" title="Изменить"><i class="icon-pencil blue"></i></A>
